<UserControl x:Class="rupban.loginmodule.Views.AdminPanelView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Border Height="192" Width="356" BorderBrush="DarkKhaki" BorderThickness="2">
        <Grid>
            <StackPanel>
            <Button   Name="button1" VerticalAlignment="Top" Command="{Binding Path=ViewProjectListCommand}" CommandParameter="Test">Current Project List</Button>
                <Button   VerticalAlignment="Top" Command="{Binding Path=CreateNewProjectCommand}" CommandParameter="Test">Create New Project</Button>
                <Button   VerticalAlignment="Top" Command="{Binding Path=CreateNewTemplateCommand}" CommandParameter="Test">Create a New Table Template</Button>
            </StackPanel>
        </Grid>
    </Border>
</UserControl>
